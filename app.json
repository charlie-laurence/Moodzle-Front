{
  "expo": {
    "name": "moodzle",
    "slug": "moodzle",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/moodz_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#F0FFF9"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "##F0FFF9"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
